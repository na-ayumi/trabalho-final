datasource db {
  provider = "sqlite"
  url      = "file:./database/db.db"
}

generator client {
  provider = "prisma-client-js"
}

model Car {
  id           String  @id
  licensePlate String  @unique
  available    Boolean
}

model Rental {
  id        String    @id
  carId     String
  startDate DateTime
  endDate   DateTime
  createAt  DateTime  @default(now())
}
